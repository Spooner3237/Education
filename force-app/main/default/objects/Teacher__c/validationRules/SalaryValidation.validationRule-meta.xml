<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SalaryValidation</fullName>
    <active>true</active>
    <errorConditionFormula>Salary__c &lt; 3000 || Salary__c &gt;10000</errorConditionFormula>
    <errorMessage>Salary 3000 ile 10000 arasında olmalıdır.</errorMessage>
</ValidationRule>
